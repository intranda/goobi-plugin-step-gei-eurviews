<?xml version="1.0" encoding="UTF-8"?>
<config_plugin>
    <elements>
        <docstruct>Autorentext</docstruct>
        <docstruct>Karte</docstruct>
        <docstruct>Bild</docstruct>
        <docstruct>Graphik</docstruct>
        <docstruct>Dokument</docstruct>

        <language>ger</language>
        <language>eng</language>
        <language>original</language>


        <person>Author</person>
        <person>Editor</person>
        <publisher>Verlag</publisher>

        <placeholder>kein Platzhalter</placeholder>
        <placeholder>Standardplatzhalter</placeholder>
        <placeholder>in Bearbeitung</placeholder>
        <placeholder>in Lizensierung</placeholder>
    </elements>

    <images>
        <docstruct>Quelle</docstruct>
        <docstruct>Titel</docstruct>
        <docstruct>Impressum</docstruct>
        <docstruct>Inhaltsverzeichnis</docstruct>
        <docstruct>Vorwort</docstruct>
    </images>

    <licences>
        <licence>https://creativecommons.org/licenses/by-nc-sa/3.0/de/</licence>
        <licence>https://creativecommons.org/licenses/by-sa/3.0/de/</licence>
        <licence>https://creativecommons.org/licenses/by-nc-nd/3.0/de/</licence>
        <licence>restricted</licence>
    </licences>


    <topicList>
        <topic>
            <name language="de">Zukunftsvisionen</name>
            <name language="en">visions of the future</name>

            <keyword gnd="123" wv="456">
                <name language="de">Fortschritt</name>
                <name language="en">progress</name>
            </keyword>
            
            <keyword gnd="987" wv="654">
                <name language="de">Metropole</name>
                <name language="en">metropolis</name>
                <synonym language="de">Gro√üstadt</synonym>
            </keyword>
            
        </topic>

        <topic>
            <name language="de">Gewaltsame Konflikte und Frieden</name>
            <name language="en">Violent Conflicts</name>

            <keyword gnd="123" wv="456">
                <name language="de">Erster Weltkrieg</name>
                <name language="en">First World War</name>
                <synonym language="de">WWI</synonym>
                <synonym language="en">WWI</synonym>
                <synonym language="en">The Great War</synonym>
            </keyword>
            
            <keyword>
                <name language="de">Kolonialkrieg</name>
                <name language="en">Colonial War</name>
            </keyword>
            
            <keyword>
                <name language="de">Revolution</name>
                <name language="en">revolution</name>
            </keyword>
            
        </topic>
    </topicList>

	<presentation>
    	<url>http://localhost:8080/viewer</url>
    </presentation>
    <normdata>
    	<keys>
			<key>NORM_IDENTIFIER</key>
    		<key>URI</key>
    		<key>NORM_NAME</key>
    		<key>NORM_ORGANIZATION</key>
    		<key>NORM_ALTNAME</key>
    		<key>NORM_ABBREVIATION</key>
    		<key>NORM_PREVIOUSNAME</key>
    		<key>NORM_LATERNAME</key>
    		<key>NORM_PLACEASSOCIATED</key>
    		<key>NORM_LIFEPERIOD</key>
    		<key>NORM_PROFESSION</key>
    		<key>NORM_EXPLANATORYTEXT</key>
    	</keys>
    </normdata>
</config_plugin>

